<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="female_patient_action" model="ir.actions.act_window">
        <field name="name">female Patient</field>
        <field name="res_model">hospital.patient</field>
        <field name="context">{'default_gender':'female' , 'default_age':25}</field>
        <field name="domain">[('gender','=','female'),('age','&gt;',6)]</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create Your First Female Patient !!!
            </p>
        </field>
    </record>
</odoo>